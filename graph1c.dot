graph id3_tree {
    A [label="A" shape=box];
    B [label="B" shape=box];
    C [label="C" shape=box];
    BC [label="B/C" shape=box];
    
    leaf1 [label="0"];
    leaf2 [label="0"];
    leaf3 [label="1"];
    leaf4 [label="0"];
    leaf5 [label="1"];

    A -- B [label="1"];
    A -- BC [label="0"];
    B -- leaf1 [label="1"];
    B -- C [label="0"];
    C -- leaf2 [label="1"];
    C -- leaf3 [label="0"];
    BC -- leaf4 [label="0"];
    BC -- leaf5 [label="1"];
}
